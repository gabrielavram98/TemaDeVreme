<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>

<AnchorPane fx:id="OkButton" onMouseClicked="#OkButtonHandler" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="ro.mta.se.lab.controller.weatherController">
   <children>
      <Pane layoutX="11.0" layoutY="11.0" prefHeight="189.0" prefWidth="577.0">
         <children>
            <ImageView fx:id="Image" fitHeight="150.0" fitWidth="200.0" layoutX="14.0" layoutY="14.0" pickOnBounds="true" preserveRatio="true" />
            <TabPane id="TabMenu" layoutX="295.0" layoutY="7.0" prefHeight="143.0" prefWidth="200.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Weather">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <Label id="Main" layoutX="8.0" layoutY="14.0" prefHeight="17.0" prefWidth="182.0" text="Main " />
                              <Label id="Description" layoutX="7.0" layoutY="40.0" prefHeight="17.0" prefWidth="189.0" text="Description" />
                              <Label id="Sunrise" layoutX="8.0" layoutY="66.0" prefHeight="17.0" prefWidth="181.0" text="Sunrise" />
                              <Label id="Sunset" layoutX="8.0" layoutY="92.0" prefHeight="17.0" prefWidth="181.0" text="Sunset" />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
                <Tab text="Details">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <Label id="MaxTemp" layoutX="8.0" layoutY="14.0" prefHeight="17.0" prefWidth="160.0" text="Max temp" />
                              <Label id="FeelsLike" layoutX="8.0" layoutY="40.0" prefHeight="17.0" prefWidth="159.0" text="Feels like" />
                              <Label id="MinTemp" layoutX="9.0" layoutY="68.0" prefHeight="17.0" prefWidth="171.0" text="Min temp " />
                              <Label id="Preassure" layoutX="8.0" layoutY="92.0" prefHeight="17.0" prefWidth="172.0" text="Preassure" />
                           </children>
                        </AnchorPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
         </children></Pane>
      <ComboBox fx:id="countrycombo" layoutX="82.0" layoutY="200.0" onAction="#CountryHandler" prefWidth="150.0" promptText="Select a country" />
      <ComboBox fx:id="citycombo" layoutX="367.0" layoutY="200.0" onAction="#CityHandler" prefWidth="150.0" promptText="Select a city" />
      <Button fx:id="OkButtonDown" layoutX="291.0" layoutY="270.0" mnemonicParsing="false" onAction="#OkButtonHandler" text="OK" />
   </children>
</AnchorPane>
